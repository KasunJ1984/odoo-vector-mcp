    "xmlrpc": "^1.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/xmlrpc": "^1.3.10",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
